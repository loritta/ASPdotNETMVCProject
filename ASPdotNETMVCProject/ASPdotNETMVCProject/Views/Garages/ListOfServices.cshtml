@model  ASPdotNETMVCProject.ViewModels.ServicesInGarageViewModel


@{
    ViewBag.Title = "ListOfServices";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Garage.Name</h2>
@if (!Model.Services.Any())
{
    <p>There are no services available.</p>
}
else
{
    <ul>

        @foreach (var ph in Model.Services)
        {

            <li>@ph.Description</li>


        }
    </ul>
}
<p>

    @Html.ActionLink("Back to List", "Index")
</p>